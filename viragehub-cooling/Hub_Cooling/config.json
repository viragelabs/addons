{
  "name": "VirageHub Active Cooling",
  "version": "2.02",
  "slug": "virage_hub_temp",
  "description": "Keeping your VirageHub cool.",
  "arch": ["armhf", "armv7", "aarch64"],
  "auth_api": true,
  "homeassistant_api": true,
  "hassio_role": "homeassistant",
  "hassio_api": true,
  "devices": ["/dev/i2c-1"],
  "url": "https://github.com/viragelabs/addons/tree/main/viragehub-cooling/Hub_Cooling",
  "map": ["config", "ssl", "addons", "backup", "share", "media"],
  "options": {
    "Celsius/Fahrenheit": "C",
    "Low Speed Temp": 40,
    "Medium Speed Temp": 50,
    "High Speed Temp": 60,
    "Use Quiet Profile": false,
    "Create Fan Speed & Temp entities in Home Assistant": true
  },
  "schema": {
    "Celsius/Fahrenheit": "str",
    "Low Speed Temp": "int(0,255)",
    "Medium Speed Temp": "int(0,255)",
    "High Speed Temp": "int(0,255)",
    "Use Quiet Profile": "bool",
    "Create Fan Speed & Temp entities in Home Assistant": "bool"
  }
}
